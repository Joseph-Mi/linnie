/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2712";

    fragment@0 {
        target = <&i2c0>;
        __overlay__ {
            status = "okay";
            #address-cells = <1>;
            #size-cells = <0>;

            camera@10 {
                compatible = "ovti,ov5647";
                reg = <0x10>;
                status = "okay";

                port {
                    camera_0: endpoint {
                        remote-endpoint = <&csi1_ep>;
                        clock-lanes = <0>;
                        data-lanes = <1 2>;
                        clock-noncontinuous;
                        link-frequencies = /bits/ 64 <297000000>;
                    };
                };
            };
        };
    };

    fragment@1 {
        target = <&csi1>;
        __overlay__ {
            status = "okay";

            port {
                csi1_ep: endpoint {
                    remote-endpoint = <&camera_0>;
                };
            };
        };
    };
};


